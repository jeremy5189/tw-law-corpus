    "law_data": [
        {
            "note": "",
            "rule_no": "第一條",
            "content": "　　本法依憲法第六十一條制定之。",
            "relates": {
                "引用條文": [
                    {
                        "law_no": "04101",
                        "law_name": "中華民國憲法",
                        "numbers": [
                            "第六十一條"
                        ]
                    }
                ]
            }
        },
        {
            "note": "",
            "rule_no": "第二條",
            "content": "　　行政院行使憲法所賦予之職權。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第三條",
            "content": "　　行政院設下列各部：\n　　一、內政部。\n　　二、外交部。\n　　三、國防部。\n　　四、財政部。\n　　五、教育部。\n　　六、法務部。\n　　七、經濟及能源部。\n　　八、交通及建設部。\n　　九、勞動部。\n　　十、農業部。\n　　十一、衛生福利部。\n　　十二、環境資源部。\n　　十三、文化部。\n　　十四、科技部。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第四條",
            "content": "　　行政院設下列各委員會：\n　　一、國家發展委員會。\n　　二、大陸委員會。\n　　三、金融監督管理委員會。\n　　四、海洋委員會。\n　　五、僑務委員會。\n　　六、國軍退除役官兵輔導委員會。\n　　七、原住民族委員會。\n　　八、客家委員會。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第五條",
            "content": "　　行政院置政務委員七人至九人，特任。\n　　政務委員得兼任前條委員會之主任委員。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第六條",
            "content": "　　行政院設行政院主計總處及行政院人事行政總處。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第七條",
            "content": "　　行政院設中央銀行。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第八條",
            "content": "　　行政院設國立故宮博物院。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第九條",
            "content": "　　行政院設下列相當中央二級獨立機關：\n　　一、中央選舉委員會。\n　　二、公平交易委員會。\n　　三、國家通訊傳播委員會。",
            "relates": {
                "被引用條文": [
                    {
                        "law_no": "01427",
                        "law_name": "促進轉型正義條例",
                        "numbers": [
                            "第二條"
                        ]
                    }
                ]
            }
        },
        {
            "note": "",
            "rule_no": "第十條",
            "content": "　　行政院院長綜理院務，並指揮監督所屬機關及人員。\n　　行政院院長因事故不能視事時，由副院長代理其職務。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第十一條",
            "content": "　　行政院院長得邀請或指定有關人員列席行政院會議。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第十二條",
            "content": "　　行政院置秘書長一人，特任，綜合處理本院幕僚事務；副秘書長二人，其中一人職務比照簡任第十四職等，襄助秘書長處理本院幕僚事務。\n　　行政院置發言人一人，特任，處理新聞發布及聯繫事項，得由政務職務人員兼任之。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第十三條",
            "content": "　　行政院各職稱之官等職等及員額，另以編制表定之。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第十四條",
            "content": "　　行政院為處理特定事務，得於院內設專責單位。",
            "relates": {}
        },
        {
            "note": "",
            "rule_no": "第十五條",
            "content": "　　本法自中華民國一百零一年一月一日開始施行。",
            "relates": {}
        }
    ],
    "law_reasons": {
        "第一條": "　　本條未修正。",
        "第二條": "　　本條未修正。",
        "第三條": "　　照黨團協商條文通過。",
        "第四條": "　　照黨團協商條文通過。",
        "第五條": "　　照黨團協商條文通過。",
        "第六條": "　　照黨團協商條文通過。",
        "第七條": "　　照黨團協商條文通過。",
        "第八條": "　　照黨團協商條文通過。",
        "第九條": "　　照黨團協商條文通過。",
        "第十條": "　　一、條次變更，本條為原條文第七條移列。\n　　二、配合基準法第十七條規定修正。",
        "第十一條": "　　條次變更，本條為原條文第八條移列，並酌作文字修正。",
        "第十二條": "　　照黨團協商條文通過。",
        "第十三條": "　　一、條次變更，本條為原條文第十一條移列。\n　　二、有關職務列等及員額配置等，將另以編制表定之，爰刪除原條文有關員額之規定。",
        "第十四條": "　　行政院為處理特定事務，得於院內設專責單位，並於行政院處務規程明定。",
        "第十五條": "　　照黨團協商條文通過。"
    },
